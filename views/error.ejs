<%block('title', 'Error')%>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <title><%=blocks.title%></title>

</head>
<body>

<% if (message) { %>
<h1><div class="alert" align="center">
    <% if (status) { %>
        <%= status %>
    <% } %>
    <%= message %>
</div></h1>
<% } %>


</body>
</html>